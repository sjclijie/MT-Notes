<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>

	<% include ../tmpl/header_meta.ejs %>

	<title><%= pageTitle %> - <%= settings.site.name %></title>

	<% include ../tmpl/header_pub.ejs %>

	<link rel="stylesheet" href="/public/css/css-page/docs-multi.min.css">

	<!-- GLOBAL JAVASCRIPT -->
	<script>
		JS_PAGE_NAME = 'none';
	</script>

	<% MENU_ACTIVE = 'Docs'; %>

</head>
<body>

<div class="wrap">

	<div class="layout">

		<% include ../tmpl/layout_header.ejs %>

		<div class="main">
			<div class="document">
				<% if (pageContent.render){ %>

				<!--<h2 class="title">Documents - <%= pageContent.title %> </h2>-->
				<h4>List For Documents & Directory</h4>
				<hr/>

				<div class="content">
					<% if(pageContent.content.constructor == Array){ %>
					<ul>
						<% pageContent.content.forEach(function(obj){ %>
							<% if(obj.name !== '.gitignore'){ %>
								<li>
									<a href="/docs/<%= encodeURI(obj.path.toString()) %>"><%= obj.name.toString() %></a>
								</li>
							<% } %>
						<% }); %>
					</ul>
					<% }else{ %>
					<%= pageContent.content.message + ' . From ' + pageContent.content.documentation_url %>
					<% } %>
				</div>

				<% }else{ %>

				<h3 class="title">Can't get ?</h3>

				<div class="content">
					<pre>Something wrong, you may try again.</pre>
					<%- pageContent.content %>
				</div>

				<% } %>

				<hr/>
			</div>
		</div>
		<div class="footer">
			<div class="row inner">
				<div class="col-12 center text-right">
					All Documents are hosted on <i class="fa fa-github"> Github</i> .
				</div>
			</div>
		</div>
	</div>

</div>

<% include ../tmpl/footer_pub.ejs %>
<% include ../tmpl/footer_sea.ejs %>

</body>
</html>

